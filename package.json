{
  "name": "yankee-doodle",
  "version": "2.0.0",
  "description": "Yank values from a given object via a specified schema",
  "author": "marziply contact@marziply.dev",
  "scripts": {
    "test": "jest ./tests",
    "build": "parcel build ./index.js --no-cache",
    "lint": "eslint ./src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marziply/yankee-doodle.git"
  },
  "keywords": [
    "yank",
    "yanked",
    "pick",
    "picked",
    "pluck",
    "plucked",
    "schema"
  ],
  "bugs": {
    "url": "https://github.com/marziply/yankee-doodle/issues"
  },
  "engines": {
    "node": ">=14.x",
    "browsers": "> 0.25%"
  },
  "main": "dist/yankee-doodle.min.js",
  "homepage": "https://github.com/marziply/yankee-doodle#readme",
  "license": "MIT",
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.9",
    "@babel/eslint-plugin": "^7.14.5",
    "eslint": "^7.32.0",
    "husky": "^4.2.1",
    "jest": "^27.0.6",
    "parcel": "^2.0.0-beta.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test"
    }
  }
}
